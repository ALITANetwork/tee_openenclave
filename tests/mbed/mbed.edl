enclave {
    include "fcntl.h"
    include "sys/types.h"
    include "sys/stat.h"
    include "myfileio.h"

    struct mbed_args {
        int skipped;
        int total;
    };

    enum string_limit {
        STRLEN = 1024
    };

    trusted {
        public int test(
            [in, string] const char* in_testname,
            [out, size=1024] char* out_testname,
            [out] mbed_args* args);
    };

    untrusted {
        void ocall_exit(
            int arg);
    };
};
