enclave {
    from "openenclave/edl/fcntl.edl" import *;
    from "platform.edl" import *;

    trusted {
        public void Ping(
            [in, string] const char* in,
            [in, out, string] char* out,
            int out_length);

    };

    untrusted {
        void Pong(
            [in, string] const char* in,
            [in, out, string] char* out,
            int out_length);

        void Log(
            [string, in] const char* str,
            uint64_t x);        
    };
};


